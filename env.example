# TickTick OAuth2 Configuration
TICKTICK_CLIENT_ID=your_client_id_here
TICKTICK_CLIENT_SECRET=your_client_secret_here
TICKTICK_REDIRECT_URI=https://your-app.vercel.app/auth/callback

# Fallback Authentication (if OAuth2 not available)
TICKTICK_USERNAME=your_username_here
TICKTICK_PASSWORD=your_password_here

# Server Configuration
PORT=3003
NODE_ENV=development

# Database (optional - for storing processed tasks)
DATABASE_URL=sqlite:./ticktick_sync.db 